{
  "name": "3PO#fail",
  "version": "1.1",
  "description": "What happens to your site when 3rd party sites are down",
  "icons": {
    "16": "icon-on.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": ["webRequest", "webRequestBlocking", "*://*/*"],
  "background": {
    "scripts": ["urls.js", "background.js"]
  },
  "browser_action": {
    "name": "fail",
    "default_title": "Click to blackhole 3rd party scripts",
    "default_icon": "icon-off.png"
  },
  "manifest_version": 2
}
